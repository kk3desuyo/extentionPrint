{
  "manifest_version": 3,
  "name": "printExtention",
  "version": "1.0.1",
  "background": {
    "service_worker": "service-worker.js",
    "type": "module"
  },
  "content_scripts": [
    {
      "matches": [
        "https://xxxxxxxxxxxxxxxxxxxxxxxxx"
      ],
      "js": ["content.js"]
    }
  ],
  "web_accessible_resources": [
    {
      "resources": ["img/*"],
      "matches": ["<all_urls>"]
    }
  ],
  "description": "印刷サイトの複数投入を可能にする拡張機能です。"
}
